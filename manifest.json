{
    "name": "Blankpad",
    "version": "1.1",
    "manifest_version": 3,
    "description": "A simple notepad extension for Google Chrome",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
      },
    "action":{
        "default_icon": "img/icon48.png",
        "default_title": "Blankpad"
    },
    "background":{
      "service_worker": "js/background.js"
    },
    "commands": {
        "_execute_action": {
          "description": "Open a new blank tab",
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
    }
}